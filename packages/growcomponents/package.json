{
  "name": "@yaddycomponents/growcomponents-module",
  "version": "1.0.0",
  "description": "Shared React component library with Antd theme for Module Federation CRM",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./theme": {
      "import": "./dist/theme.js",
      "types": "./dist/theme.d.ts"
    },
    "./components": {
      "import": "./dist/components/index.js",
      "types": "./dist/components/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "prepublishOnly": "pnpm build"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "antd": "^5.12.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yaddycomponents/Module-CRM.git",
    "directory": "packages/growcomponents"
  },
  "keywords": [
    "react",
    "antd",
    "components",
    "module-federation",
    "crm"
  ],
  "author": "yaddycomponents",
  "license": "MIT"
}
